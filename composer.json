{
    "name": "edixon-alberto/api-dicapp",
    "description": "API for app movile",
    "license": "GPL-3.0-only",
    "type": "project",
    "authors": [
        {
            "name": "Edixon Alberto",
            "email": "edixonalbertto@gmail.com",
            "homepage": "https://www.linkedin.com/in/edixonalberto"
        }
    ],
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "require": {
        "php": "~7.3",
        "sendgrid/sendgrid": "~7",
        "firebase/php-jwt": "^5.0",
        "namnv609/php-onesignal-sdk": "^1.0",
        "facebook/graph-sdk": "^5.7",
        "jenssegers/blade": "^1.2"
    },
    "require-dev": {
        "vlucas/phpdotenv": "^3.4"
    },
    "autoload": {
        "classmap": [
            "api/libraries",
            "api/modules"
        ],
        "psr-4": {
            "V2\\": "api/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}